{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI: mcp_server",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "mcp_server.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "8000",
        "--reload"
      ],
      "jinja": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}

